    "@remix-run/react": "1.13.0",
    "@remix-run/vercel": "1.13.0",
    "@vercel/analytics": "^0.1.10",
    "@vercel/node": "2.9.8",
    "highlight.js": "^11.7.0",
    "isbot": "^3.6.6",
    "marked": "4.2.12",
    "notion-to-md": "2.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "0.31.3",
    "web-vitals": "^3.1.1",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.13.0",
