    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint ./app --ext .ts,.tsx",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@notionhq/client": "2.2.3",
    "@radix-ui/react-hover-card": "1.0.4",
